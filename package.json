{
  "name": "not-coolors",
  "description": "Insanely fast color palette generator!",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "keywords": [
    "nextjs",
    "react",
    "tailwindcss",
    "javascript",
    "palette",
    "color",
    "generator"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prebuild": "pnpm run lint",
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "next-lint": "next lint",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write ."
  },
  "dependencies": {
    "next": "^15.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.19.0",
    "eslint": "^9",
    "eslint-config-next": "15.2.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "postcss": "8.5.2",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "3.4.17"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  },
  "prettier": {
    "semi": true,
    "tabWidth": 2,
    "printWidth": 80,
    "bracketSpacing": true,
    "singleQuote": false,
    "jsxSingleQuote": false,
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  }
}
